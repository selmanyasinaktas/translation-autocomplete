  "description": "Automatically detect missing translations and fill them using AI-powered translation APIs",
  "repository": {
    "type": "git",
    "url": "https://github.com/selmanyasin/translation-autocomplete.git"
  },
  "keywords": [
    "i18n",
    "translation",
    "localization",
    "missing translations"
  ],
